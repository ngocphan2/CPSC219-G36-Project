<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="300.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="application.ExpensesController">
   <children>
      <BorderPane prefHeight="25.0" prefWidth="100.0">
         <right>
            <Button mnemonicParsing="false" onAction="#goToController" text="Done" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </Button>
         </right>
         <center>
            <Label text="Label" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="60.0" />
               </BorderPane.margin>
            </Label>
         </center>
         <VBox.margin>
            <Insets right="10.0" top="10.0" />
         </VBox.margin>
      </BorderPane>
      <Label fx:id="errorLabel" TextFill="Red" text="Label">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <BorderPane prefHeight="50.0" prefWidth="200.0">
         <left>
            <Button mnemonicParsing="false" text="Add More Activities" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="10.0" />
               </BorderPane.margin>
            </Button>
         </left>
         <right>
            <Button mnemonicParsing="false" text="Select Another Date" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets right="10.0" />
               </BorderPane.margin>
            </Button>
         </right>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </BorderPane>
      <ScrollPane prefHeight="200.0" prefWidth="200.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
        </content>
      </ScrollPane>
   </children>
</VBox>
