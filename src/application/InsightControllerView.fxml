<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.InsightController">
   <children>
      <Label text="Financial Insight">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label fx:id="overViewTitleLabel" text="Overview">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="netIncomeLabel" text="">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="netIncomeInsightLabel" textFill="Red" text="Label">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="savingsRecommendation" text="Professional financial advisors recommend that the ideal savings percentage is 20%">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="savingsPercentage" text="Label">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="savingsPercentageInsightLabel" text="Label">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label text="Emergency Fund should be enough to cover your expenses in 6 months without working">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="emergencyFund" text="">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="budgetTitleLabel" text="Label">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label fx:id="budgetInsightLabel" textFill="Red" text="Label">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label text="Advanced Analysis">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Button mnemonicParsing="false" onAction="#goToController" text="Redo" />
   </children>
</VBox>
